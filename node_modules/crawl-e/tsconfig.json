{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "outDir": "lib",
    "typeRoots": [
      "./typings", 
      "./node_modules/@types"
    ],
    "allowJs": false,
    "declaration": true,
    "removeComments": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.*test.ts"
  ],
  "typedocOptions": {
    "inputFiles": [
      "./src"
    ],
    "exclude": [
      "**/*test.ts",
      "src/config-schema.ts",
      "src/MethodCallLogger.ts"
    ],
    "excludeExternals": true,
    "mode": "file",
    "name": "CRAWLÂ·E",
    "includeVersion": true,
    "readme": "./README.typedocs.md",
    "out": "docs/typedocs",
    "excludePrivate": true,
    "excludeNotExported": false,
    "disableSources": true,
    "stripInternal": false,
    "categoryOrder": ["Main", "Model", "Context", "RequestMaker", "HTML Parsing", "*"]
  }
}